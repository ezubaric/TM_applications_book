@Article{	  zaki-05,
  abstract	= {{Mining frequent trees is very useful in domains like
		  bioinformatics, Web mining, mining semistructured data,
		  etc. We formulate the problem of mining (embedded) subtrees
		  in a forest of rooted, labeled, and ordered trees. We
		  present TreeMiner, a novel algorithm to discover all
		  frequent subtrees in a forest, using a new data structure
		  called scope-list. We contrast TreeMiner with a pattern
		  matching tree mining algorithm (PatternMatcher), and we
		  also compare it with TreeMinerD, which counts only distinct
		  occurrences of a pattern. We conduct detailed experiments
		  to test the performance and scalability of these methods.
		  We also use tree mining to analyze RNA structure and
		  phylogenetics data sets from bioinformatics domain.}},
  author	= {Zaki, M. J.},
